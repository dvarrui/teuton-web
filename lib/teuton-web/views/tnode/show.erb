
<div class="page-header">
  <h2>Show: <%= @test[:dirname] %></h2>
  <p><a href="/tnode">Back</a> |
     <a href="/tnode/test/<%= @test[:id] %>/raw">Raw</a> |
     Show
  </p>
</div>

<div class="row">
  <div class="col-sm-12">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">Test files <span class="badge"><%= @test[:files].size.to_s%></span></h3>
      </div>
      <div class="panel-body">
<!--             <div class="col-md-6">-->
        <table class="table table-condensed">
          <tbody>
            <% @test[:files].each do |filepath| %>
              <tr>
                <td><a href="/tnode/raw/<%=filepath_to_string(filepath)%>"><%=filepath%></a></td>
              </tr>
            <% end %>
          </tbody>
        </table>
<!--             </div>-->

      </div>
    </div>
  </div>
</div>
