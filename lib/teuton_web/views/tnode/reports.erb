
<div class="page-header">
  <h2>Reports: <%= @test[:dirpath] %></h2>
  <p><a href="/tnode">Back</a> |
     <a href="/tnode/show/<%= @test[:id] %>">Show</a> |
     <a href="/tnode/execute/<%= @test[:id] %>">Execute</a> |
     <a href="/tnode/resume/<%= @test[:id] %>">Resume</a> |
     <a href="/tnode/params/<%= @test[:id] %>">Params</a> |
     Reports
  </p>
</div>

<div class="row">
  <div class="col-sm-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Output reports <span class="badge"><%=@reports[:files].size.to_s%> files</span></h3>
      </div>
      <div class="panel-body">
        <table class="table table-condensed">
          <tbody>
            <% @reports[:files].each do |filepath| %>
            <tr>
              <td><a href="/tnode/raw/<%=f2s(filepath)%>"><%=filepath[@reports[:dirpath].size + 1,filepath.size]%></a></td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
