
<div class="page-header">
  <h2>Report file: <%= params[:filename] %></h2>
  <p>Directory: <span class="label label-default"><%= Dir.pwd %></span></p>
  <p>
    <a href="/snode">Back</a> |
    <a href="/snode/report/<%=params[:filename]%>/raw">Raw</a> |
    <a href="/snode/report/<%=params[:filename]%>/config">Config</a> |
    <a href="/snode/report/<%=params[:filename]%>/results">Results</a> |
    Logs |
    <a href="/snode/report/<%=params[:filename]%>/targets">Target list</a>
  </p>
</div>

<div class="row">

  <div class="col-sm-10">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Logs <span class="badge"><%= @data[:logs].size.to_s%></span></h3>
      </div>
      <div class="panel-body">
<!--             <div class="col-md-6">-->
        <table class="table table-condensed">
          <tbody>
            <% @data[:logs].each do |log| %>
              <tr>
                <td><%= log %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
<!--             </div>-->

      </div>
    </div>
  </div>
</div>
